<Page x:Class="BeyonSense.Navigation_Page"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BeyonSense"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Navigation_Page">

    <StackPanel Background="White" Orientation="Horizontal">
        <Button x:Name="labeling" Content="Labeling" Height="200" Width="200" Margin="50"
                    Click="Labeling_Click"/>
        <Button x:Name="Train" Content="Train" Height="200" Width="200"/>
        <Button x:Name="Test" Content="Test" Height="200" Width="200" Margin="50"/>
        
    </StackPanel>


</Page>
